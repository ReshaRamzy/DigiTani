    <fragment
        android:id="@+id/navigation_scan"
        android:name="com.example.myapplication.ui.scan.ScanFragment"
        android:label="fragment_scan"
        tools:layout="@layout/fragment_scan" />
    <fragment
        android:id="@+id/navigation_tanaman"
        android:name="com.example.myapplication.ui.tanaman.TanamanFragment"
        android:label="fragment_tanaman"
